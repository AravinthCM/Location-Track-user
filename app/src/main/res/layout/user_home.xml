<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingStart="0dp">


    <androidx.drawerlayout.widget.DrawerLayout
        android:id="@+id/drawerLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical"
        android:padding="0dp"
        tools:context=".HomePageActivity"
        tools:openDrawer="start">

        <ScrollView
            android:id="@+id/android_large___1"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:id="@+id/content_container"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <androidx.coordinatorlayout.widget.CoordinatorLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.google.android.material.appbar.MaterialToolbar
                        android:id="@+id/materialToolbar"
                        android:layout_width="match_parent"
                        android:layout_height="96dp"
                        android:background="@color/white"
                        android:paddingStart="15dp"
                        app:buttonGravity="center_vertical"
                        app:navigationIcon="@drawable/img_menu"
                        app:title="Tracky"
                        app:titleCentered="true"
                        app:titleMarginStart="40dp"
                        app:titleMarginTop="10dp"
                        app:titleTextColor="@color/black" />

                    <FrameLayout
                        android:id="@+id/frameLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="?attr/actionBarSize" />

                </androidx.coordinatorlayout.widget.CoordinatorLayout>


                <LinearLayout
                    android:id="@+id/page_android_large___1_ek1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:paddingStart="15dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/hi_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="20dp"
                            android:layout_weight="1"
                            android:fontFamily="sans-serif-medium"
                            android:text="@string/hi_name_string"
                            android:textColor="@color/hi_name_color"
                            android:textSize="25sp" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_marginTop="20dp"
                            android:layout_weight="1"
                            android:layout_marginStart="50dp"
                            android:gravity="center"
                            android:src="@drawable/profileeee" />
                    </LinearLayout>


                    <TextView
                        android:id="@+id/welcome_to_home"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:text="@string/welcome_to_home_string"
                        android:textColor="@color/hi_name_color"
                        android:textSize="18sp" />


                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="60dp"
                        android:layout_marginStart="5dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginEnd="10dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="10dp">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@color/white" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginStart="10dp"
                            android:text="ANNOUNCEMENTS:" />

                    </androidx.cardview.widget.CardView>


                    <TextView
                        android:id="@+id/travel"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/rectangle_2"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="15dp"
                        android:fontFamily="sans-serif-medium"
                        android:text="@string/travel_string"
                        android:textColor="@color/hi_name_color"
                        android:textSize="25dp" />

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">


                            <LinearLayout
                                android:id="@+id/routeTrack"
                                android:layout_width="160dp"
                                android:layout_height="181dp"
                                android:layout_below="@+id/travel"
                                android:layout_alignParentStart="true"
                                android:layout_marginTop="16dp"
                                android:background="@drawable/rectangle_3_shape"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="70dp"
                                    android:layout_height="70dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="20dp"
                                    android:layout_marginEnd="20dp"
                                    android:src="@drawable/frontbuss" />

                                <TextView
                                    android:id="@+id/track_your_bus"
                                    android:layout_width="110dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="14dp"
                                    android:layout_marginEnd="0dp"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="@string/track_your_bus_string"
                                    android:textColor="@color/_bg__android_large___1_ek2_color"
                                    android:textSize="20dp" />

                            </LinearLayout>


                            <androidx.cardview.widget.CardView
                                android:id="@+id/cardREQ"
                                android:layout_width="160dp"
                                android:layout_height="181dp"
                                android:layout_marginStart="5dp"
                                android:layout_marginTop="10dp"
                                android:layout_marginEnd="5dp"
                                android:elevation="100dp"
                                app:cardCornerRadius="40dp">

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@color/white" />

                                <ImageView
                                    android:layout_width="70dp"
                                    android:layout_height="70dp"
                                    android:layout_marginStart="30dp"
                                    android:layout_marginTop="20dp"
                                    android:layout_marginEnd="20dp"
                                    android:src="@drawable/request" />

                                <TextView
                                    android:id="@+id/service_request"
                                    android:layout_width="110dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="30dp"
                                    android:layout_marginTop="85dp"
                                    android:layout_marginEnd="29dp"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="@string/service_request_string"
                                    android:textColor="@color/hi_name_color"
                                    android:textSize="20dp" />


                            </androidx.cardview.widget.CardView>


                        </LinearLayout>


                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <androidx.cardview.widget.CardView
                                android:layout_width="160dp"
                                android:layout_height="120dp"
                                android:layout_marginStart="5dp"
                                android:layout_marginTop="0dp"
                                android:layout_marginEnd="5dp"
                                android:elevation="100dp"
                                app:cardCornerRadius="40dp">

                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:background="@color/white" />

                                <ImageView
                                    android:id="@+id/vector"
                                    android:layout_width="33dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="35dp"
                                    android:src="@drawable/profileeee" />

                                <TextView
                                    android:id="@+id/your_profile"
                                    android:layout_width="wrap_content"
                                    android:layout_height="25.8dp"
                                    android:layout_gravity="center_horizontal|center"
                                    android:layout_marginStart="25dp"
                                    android:fontFamily="sans-serif-medium"
                                    android:gravity="left"
                                    android:text="@string/your_profile_string"
                                    android:textColor="@color/hi_name_color"
                                    android:textSize="15dp" />


                            </androidx.cardview.widget.CardView>


                            <LinearLayout
                                android:id="@+id/trackBus"
                                android:layout_width="160dp"
                                android:layout_height="181dp"
                                android:layout_below="@+id/rectangle_4_shape"
                                android:layout_alignParentEnd="true"
                                android:layout_marginStart="10dp"
                                android:layout_marginTop="10dp"
                                android:background="@drawable/rectangle_5_shape"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="70dp"
                                    android:layout_height="70dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="20dp"
                                    android:layout_marginStart="10dp"
                                    android:layout_marginEnd="20dp"
                                    android:src="@drawable/route" />

                                <TextView
                                    android:id="@+id/routes_of_bus"
                                    android:layout_width="110dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="25dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginEnd="0dp"
                                    android:fontFamily="sans-serif-medium"
                                    android:text="@string/routes_of_bus_string"
                                    android:textColor="@color/hi_name_color"
                                    android:textSize="20dp" />


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/rectangle_7"
                                android:layout_width="160dp"
                                android:layout_height="120dp"
                                android:layout_below="@+id/travel"
                                android:layout_alignParentEnd="true"
                                android:layout_marginStart="10dp"
                                android:layout_marginTop="10dp"
                                android:layout_toRightOf="@+id/rectangle_3"
                                android:background="@drawable/rectangle_7_shape"
                                android:orientation="vertical">

                                <ImageView
                                    android:layout_width="40dp"
                                    android:layout_height="40dp"
                                    android:layout_gravity="center"
                                    android:layout_marginTop="20dp"
                                    android:src="@drawable/logoutbtn" />

                                <TextView
                                    android:id="@+id/logout"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:fontFamily="sans-serif-medium"
                                    android:gravity="center"
                                    android:text="@string/logout_string"
                                    android:textColor="@color/white"
                                    android:textSize="18sp" />

                            </LinearLayout>

                        </LinearLayout>


                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

        </ScrollView>

         <com.google.android.material.navigation.NavigationView
             android:id="@+id/navigationView"
             android:layout_width="wrap_content"
             android:layout_height="match_parent"
             android:layout_gravity="start"
             app:headerLayout="@layout/nav_header"
             android:background="@color/white"
             app:itemIconTint="@color/black"
             app:itemTextColor="@color/black"
             app:menu="@menu/nav_menu" />

     </androidx.drawerlayout.widget.DrawerLayout>
 </LinearLayout>
